<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@../../CSS/MainStylesheet.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.MVC.Controller.AdminController">
   <children>
      <Pane prefHeight="100.0" prefWidth="1280.0" styleClass="topbar" stylesheets="@../../CSS/MainStylesheet.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="exitBtn" mnemonicParsing="false" onAction="#handleLogOut" styleClass="topbar-buttons" stylesheets="@../../CSS/MainStylesheet.css" text="Log Ud">
               <graphic>
                  <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Images/back.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="System Bold" size="30.0" />
               </font></Button>
            <ButtonBar layoutX="240.0" layoutY="6.0">
              <buttons>
                <Button fx:id="createBtn" mnemonicParsing="false" onAction="#handleAdd" styleClass="topbar-buttons" stylesheets="@../../CSS/MainStylesheet.css">
                     <graphic>
                        <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../Images/add.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="deleteBtn" mnemonicParsing="false" onAction="#handleRemove" styleClass="topbar-buttons" stylesheets="@../../CSS/MainStylesheet.css">
                     <graphic>
                        <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../Images/remove.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="editBtn" mnemonicParsing="false" onAction="#handleEdit" styleClass="topbar-buttons" stylesheets="@../../CSS/MainStylesheet.css">
                     <graphic>
                        <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../Images/edit.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
              </buttons>
            </ButtonBar>
            <Pane layoutX="230.0" prefHeight="100.0" prefWidth="10.0" styleClass="seperator" stylesheets="@../../CSS/MainStylesheet.css" />
            <Label layoutX="991.0" layoutY="32.0" text="Admin Dashboard">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TableView fx:id="studentTB" layoutX="15.0" layoutY="110.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="110.0">
        <columns>
          <TableColumn fx:id="fNameTC" minWidth="-1.0" prefWidth="181.0" text="Fornavn" />
          <TableColumn fx:id="lNameTC" prefWidth="181.0" text="Efternavn" />
            <TableColumn fx:id="schoolTC" prefWidth="237.0" text="Skole" />
            <TableColumn fx:id="emailTC" prefWidth="408.0" text="Email-Adresse" />
            <TableColumn fx:id="pWordTC" prefWidth="240.0" text="Kodeord" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
